  "version": "0.2.0",
  "configurations": [
    {
      "name": "Barcode Example",
      "type": "dart",
      "request": "launch",
      "program": "barcode/example/main.dart",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Barcode Widget",
      "type": "dart",
      "request": "launch",
      "program": "flutter/example/lib/main.dart"
    },
    {
      "name": "EAN13",
      "program": "image/bin/barcode.dart",
      "args": [
        "-t",
        "CodeEAN13",
        "123456789012"
      ],
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}"
    },{
      "name": "UPC-A",
      "program": "image/bin/barcode.dart",
      "args": [
        "-t",
        "CodeUPCA",
        "12345678901"
      ],
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}"
    }
  ]
}
